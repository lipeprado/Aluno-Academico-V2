<div class="cadastro-container">
  
  <h2 class="texto-cadastrar">Entrar</h2>
  
  <div class="texto-cadastroa clearfix">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  
    <%= f.text_field :name, autofocus: true, placeholder: "Nome", class: "inputs" %>
    <%= f.email_field :email, placeholder: "Email", class: "inputs" %>
    <%= f.password_field :password, autocomplete: "off", placeholder: "Senha", class: "inputs" %>
    
    
  
    

    <% if devise_mapping.rememberable? -%>
    <%= f.check_box :remember_me, class: "padding-up" %>
    <%= f.label :remember_me %>
    <% end -%>
    <%= f.submit "Entrar", class: "botao-cadastrar" %>

  
<% end %>

<%= render "devise/shared/links" %>

</div>

</div>